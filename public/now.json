{
    "version": 2,
    "name": "techtracker",
    "alias": "techtracker",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": ".*",
          "dest": "/index.html"
        },
        {
          "src": "^/favicon.ico$", 
          "dest": "/favicon.ico"
        }
    ]
  }